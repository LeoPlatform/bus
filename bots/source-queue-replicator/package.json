{
	"name": "source-queue-replicator",
	"version": "1.0.0",
	"description": "Replicates queues based on Queue Replication configuration",
	"logicalResource": "SourceQueueReplicator",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"config": {
		"leo": {
			"type": "bot",
			"memory": 256,
			"timeout": 300,
			"role": "SourceQueueReplicatorRole",
			"env": {
				"Resources": {
					"Region": "${AWS::Region}",
					"LeoCron": "${LeoCron}",
					"LeoEvent": "${LeoEvent}",
					"LeoS3": "${LeoS3}",
					"LeoSettings": "${LeoSettings}",
					"LeoStream": "${LeoStream}",
					"LeoSystem": "${LeoSystem}"
				},
				"AWS": {
					"AccountId": "${AWS::AccountId}",
					"region": "${AWS::Region}"
				}
			}
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {}
}
